#: activity/activity.info:2
msgid "I Know My ABCs"
msgstr "E di ABC-në Time"

#: IKnowMyABCs.py:127
msgid "Page"
msgstr "Faqe"

#: IKnowMyABCs.py:138 IKnowMyABCs.py:140
msgid "listen to the letter names"
msgstr "dëgjoni emrat e shkronjave"

#: IKnowMyABCs.py:149 IKnowMyABCs.py:153
msgid "find the letter that corresponds to the sound"
msgstr "gjeni shkronjën që i përgjigjet tingullit"

#: IKnowMyABCs.py:169 IKnowMyABCs.py:177
msgid "Click on a card to hear a sound."
msgstr "Klikoni mbi një kartë që të dëgjoni një tingull."

#: page.py:184
msgid "Click on the card that corresponds to the sound."
msgstr "Klikoni mbi kartën që i përgjigjet tingullit."

#: page.py:224
msgid "Very good!"
msgstr "Shumë mirë!"

#: page.py:227
msgid "Please try again."
msgstr "Ju lutemi, riprovoni."
